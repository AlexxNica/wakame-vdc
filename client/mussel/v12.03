MUSSEL_RESOURCES="
 backup_object
 host_node
 image
 instance
 load_balancer
 network
 security_group
 ssh_key_pair
 storage_node
"

function run_cmd() {
  local namespace=$1 cmd=$2

  case "${namespace}" in
  instance|load_balancer|image|backup_object|host_node|storage_node|network|security_group|ssh_key_pair)
    . ${BASH_SOURCE[0]%/*}/v${api_version}.d/${namespace}.sh
    ;;
  *)
    echo "$0 [namespace] [cmd]" >&2
    return 1
    ;;
  esac
}
