#------------------------
# Configuration file for sta-linux.
#------------------------

# iSCSI Qualified Name (IQN) prefix : {literal iqn}.{yyyy-mm}.{reversed domain name of the authority}
config.iqn_prefix = "iqn.2010-09.jp.wakame"

# Initiator address is IP or ALL
config.initiator_address = 'ALL'

# tgt start/stop ommand
config.start_command = "initctl start tgt"
config.stop_command = "initctl stop tgt"

